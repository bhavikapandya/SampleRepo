    <validationRules>
        <fullName>DoBrule</fullName>
        <active>true</active>
        <description>Age cannot be less than 18 years</description>
        <errorConditionFormula>OR( Age__c &lt;21,  Age__c &gt;30)</errorConditionFormula>
        <errorDisplayField>DateOfBirth__c</errorDisplayField>
        <errorMessage>“Please provide a valid Date of birth. Age is between 21-30.”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LastWorkingDay_validation</fullName>
        <active>true</active>
        <description>last working day should be graeter than joining date</description>
        <errorConditionFormula>Date_of_Joining__c  &gt; Last_Working_Day__c</errorConditionFormula>
        <errorDisplayField>Date_of_Joining__c</errorDisplayField>
        <errorMessage>Date of Joining must be less than Last working Day</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoSalaryIfInactive</fullName>
        <active>true</active>
        <errorConditionFormula>IF( IsActive__c ==false &amp;&amp;  Is_on_Notice_Period__c ==False,true, false)</errorConditionFormula>
        <errorMessage>“Salary cannot be processed for inactive employees.”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ResignationDate_Validation</fullName>
        <active>true</active>
        <description>If person is on notice period then it cannot be null</description>
        <errorConditionFormula>IF( Is_on_Notice_Period__c== TRUE &amp;&amp;  ISNULL( ResignationDate__c ),TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>ResignationDate__c</errorDisplayField>
        <errorMessage>Reasignation Date cannot be null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ResignationDate_vs_joining_date</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Date_of_Joining__c   &gt;   ResignationDate__c , TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>ResignationDate__c</errorDisplayField>
        <errorMessage>Resignation date must be greater than joining date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Roll_Id_Not_change</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Roll_id__c )</errorConditionFormula>
        <errorDisplayField>Roll_id__c</errorDisplayField>
        <errorMessage>Roll Id cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_rule</fullName>
        <active>true</active>
        <errorConditionFormula>LEN( TEXT( SSN__c ) ) &lt;&gt; 10</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>Length should be 10</errorMessage>
    </validationRules>
